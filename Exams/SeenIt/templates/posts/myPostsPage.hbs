{{> header}}
<section id="viewMyPosts">
    <div class="content">
    {{> menu}}
            <div class="post post-content">
                <h1>Your Posts</h1>
            </div>
            <div class="posts">
                {{#each posts}} 
                    {{> post}}
                {{else}}
                    <p>No posts in db!</p>
                {{/each}}
            </div>
</section>
{{> footer}}